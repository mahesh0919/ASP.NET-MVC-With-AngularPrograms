<h1>Courses we offer</h1>
<p> <b> {{text}} </b></p>
<ul>
    <li ng-repeat="course in courses track by $index">  @*use track by if there are any duplicates in list*@
        {{course}}
    </li>
</ul>

<input type="text" ng-model="inputValue" />
<button id="addCourses" ng-click="AddCourses()">Add Courses</button>